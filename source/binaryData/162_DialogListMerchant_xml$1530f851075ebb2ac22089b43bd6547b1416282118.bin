		<scenes>
			<scene>
				<nameText>
					<en>Merchant</en>
					<jp>商人</jp>
					<cn></cn>
				</nameText>
				<dialogText>
					<en>"So what is it gonna be?"</en>
					<jp>「さあ、どうする？」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="merchant" fromX="0.8" fromY="0.8" toX="0.8" toY="0.8" alpha="1" scaleX="1" scaleY="1"/>
