<?xml version="1.0" encoding="UTF-8"?>
<dialogs>

	<obj>
		<objName>GenericMeet1</objName>
		<init>
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Help!"</en>
					<jp>「誰か！」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Somebody please protect me!"</en>
					<jp>「誰か助けて！」</jp>
					<cn></cn>
				</dialogText>
			</scene>
		</scenes>
	</obj>

	<!-- GENERIC RECRUIT -->
	<obj>
		<objName>GenericRecruit1</objName>
		<init>
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes next="meetSurvivorDialog">
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Thank you! I thought I was a goner!"</en>
					<jp>「よかった！死んでしまうかと思った！」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"I was separated from my group, and next thing I know zeds were all over the place."</en>
					<jp>「仲間とはぐれて、気づいたらゾンビに囲まれていたんだ」</jp>
					<cn></cn>
				</dialogText>
			</scene>
		</scenes>
	</obj>
	
	<obj>
		<objName>GenericRecruit2</objName>
		<init>
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes next="meetSurvivorDialog">
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Other survivors! Thank God!"</en>
					<jp>「他にも生存者がいたとは！助かった」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"It's a relief to finally find another living person."</en>
					<jp>「自分以外の生きている人間に会えてうれしいよ」</jp>
					<cn></cn>
				</dialogText>
			</scene>
		</scenes>
	</obj>
	
	<obj>
		<objName>GenericRecruit3</objName>
		<init>
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes next="meetSurvivorDialog">
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"I've lost everything..."</en>
					<jp>「何もかも失くした…」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Please...take me with you! I have nowhere else to go."</en>
					<jp>「仲間に入れてもらえない？もう行くところがない…」</jp>
					<cn></cn>
				</dialogText>
			</scene>
		</scenes>
	</obj>
	<!-- GENERIC RECRUIT END -->
	
	<!-- GENERIC KICKOUT -->
	<obj>
		<objName>GenericKickOut1</objName>
		<init>
			<blinds imageName="blinds" alpha="1" />
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"I can't believe you want to kick me out!"</en>
					<jp>「追い出そうとするなんて信じられない！」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Why are you doing this?"</en>
					<jp>「どうしてこんなことを？」</jp>
					<cn></cn>
				</dialogText>
			</scene>
		</scenes>
		
		<options>
			<option>
				<name>
					<en>Kick Out</en>
					<jp>追い出す</jp>
					<cn>Kick Out3</cn>
				</name>
				<links>
					<link chance="1">confirmKickOut</link>
				</links>
			</option>
			<option>
				<name>
					<en>Cancel</en>
					<jp>キャンセル</jp>
					<cn>Cancel3</cn>
				</name>
				<links>
					<link chance="1">cancelKickOut</link>
				</links>
			</option>
		</options>
	</obj>
	
	<obj>
		<objName>confirmKickOut</objName>
		<init>
			<blinds imageName="blinds" alpha="1" />
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Fine! I'm leaving!"</en>
					<jp>「もういい！出て行く！」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
				<trigger type="object" action="kickOutSurvivor"></trigger>
			</scene>
		</scenes>
	</obj>
	
	<obj>
		<objName>cancelKickOut</objName>
		<init>
			<blinds imageName="blinds" alpha="1" />
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Phew, I thought you are going to kick me out."</en>
					<jp>「ああ、追い出されるかと思った」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
		</scenes>
	</obj>
	<!-- GENERIC KICKOUT END -->
	
	<!-- SYDNEY -->
	
	<obj>
		<objName>SydneyRecruit1</objName>
		<init>
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
			<portrait imageName="mainCharacter" texture="%MAIN-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes next="meetSurvivorDialog">
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Glad to see someone still breathing."</en>
					<jp>「まだ生きている人がいたんだ。うれしい」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%MAIN-CHAR-FULL-NAME%</en>
					<jp>%MAIN-CHAR-FULL-NAME%</jp>
					<cn>%MAIN-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"You okay? Did you get hurt?"</en>
					<jp>「大丈夫？ケガしてない？」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="0.7" fromY="0.7" toX="1" toY="0.7" alpha="0" scaleX="1" scaleY="1"/>
				<tween imageName="mainCharacter" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Yeah, I can manage...this is not my first rodeo."</en>
					<jp>「私は大丈夫…慣れてるから」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
				<tween imageName="mainCharacter" fromX="0.7" fromY="0.7" toX="1" toY="0.7" alpha="0" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"I'm a combat journalist, I've seen many wars in my line of work."</en>
					<jp>「戦争ジャーナリストなの。この仕事でたくさんの戦いを見てきた」</jp>
					<cn></cn>
				</dialogText>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"I just can't believe that war would follow me home..."</en>
					<jp>「でも、地元で戦いに巻き込まれるとは思わなかった…」</jp>
					<cn></cn>
				</dialogText>
			</scene>
		</scenes>
	</obj>
	<!-- SYDNEY END -->
	
	<!-- SCIENTIST -->
	<obj>
		<objName>ScientistRecruit1</objName>
		<init>
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
			<portrait imageName="mainCharacter" texture="%MAIN-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes next="meetSurvivorDialog">
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Hiya, uhm, strangers, can you tell me where I am exactly?"</en>
					<jp>「やあ、はじめまして。ここは座標値でいうとどのあたりかな？」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%MAIN-CHAR-FULL-NAME%</en>
					<jp>%MAIN-CHAR-FULL-NAME%</jp>
					<cn>%MAIN-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"You are still in the city, don't worry you are safe for now. Are you some kind of a doctor?"</en>
					<jp>「まだ街に人がいるとは。だがもう安全だ。医者か何かか？」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="0.7" fromY="0.7" toX="1" toY="0.7" alpha="0" scaleX="1" scaleY="1"/>
				<tween imageName="mainCharacter" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Ah, I'm sorry, my name is %TARGET-CHAR-FIRST-NAME% %TARGET-CHAR-LAST-NAME%. But, all my friends call me %TARGET-CHAR-NICK-NAME%."</en>
					<jp>「ああ、いや、私の名前は%TARGET-CHAR-FIRST-NAME%・%TARGET-CHAR-LAST-NAME%。仲間からは%TARGET-CHAR-NICK-NAME%と呼ばれているがな」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.7" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
				<tween imageName="mainCharacter" fromX="0.7" fromY="0.7" toX="1" toY="0.7" alpha="0" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"My team and I was sent by the CDC to collect data and take samples for our research."</en>
					<jp>「CDCの依頼でここに来た。研究用サンプルを収集して、データを集めるためにな」</jp>
					<cn></cn>
				</dialogText>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Our transport was attacked and I was knocked unconscious."</en>
					<jp>「だが移動の途中で私のチームが攻撃され、私は意識を失っていたようだ」</jp>
					<cn></cn>
				</dialogText>
			</scene>
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"The moment I woke up, everyone was gone."</en>
					<jp>「目を覚ますともう誰もいなかった」</jp>
					<cn></cn>
				</dialogText>
			</scene>
		</scenes>
	</obj>
	<!-- SCIENTIST END -->
	
	<!-- WORKER -->
	<obj>
		<objName>WorkerRecruit1</objName>
		<init>
			<portrait imageName="character" texture="%TARGET-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
			<portrait imageName="mainCharacter" texture="%MAIN-CHAR-PHOTO%" posX="0.7" posY="0.7" scaleX="1" scaleY="1"/>
		</init>
		<scenes next="meetSurvivorDialog">
			<scene>
				<nameText>
					<en>%TARGET-CHAR-FULL-NAME%</en>
					<jp>%TARGET-CHAR-FULL-NAME%</jp>
					<cn>%TARGET-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Stop right there! Move closer and I'll chop your head off!"</en>
					<jp>「おい止まれ！それ以上近づくと首を叩き切る！」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="1" fromY="0.7" toX="0.8" toY="0.7" alpha="1" scaleX="1" scaleY="1"/>
			</scene>
			<scene>
				<nameText>
					<en>%MAIN-CHAR-FULL-NAME%</en>
					<jp>%MAIN-CHAR-FULL-NAME%</jp>
					<cn>%MAIN-CHAR-FULL-NAME%</cn>
				</nameText>
				<dialogText>
					<en>"Whoa, take it easy! We are not infected! We are here to help."</en>
					<jp>「ちょっと、落ち着いて。感染者じゃない。助けに来たんだ」</jp>
					<cn></cn>
				</dialogText>
				<tween imageName="character" fromX="0.7" fromY="0.7" toX="1" toY="0.7" alpha="0" scaleX="1" scaleY="1"/>
